{
    "version": 2,
    "builds": [
        {
            "src": "src/server.js",
            "use": "@vercel/node"
        }
    ],
    "routes": [
        {
            "src": "/socket.io/(.*)",
            "dest": "/src/socket.js"
        },
        {
            "src": "/(.*)",
            "dest": "src/server.js"
        }
    ]
}